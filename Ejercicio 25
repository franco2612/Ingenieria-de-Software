using System;
namespace ejercicio_25_is
{
    class Program
    {
        static void Main(string[] args)
        {
            int numero;
            int resultado;
            Console.WriteLine("Ingrese un numero:");
            numero = Convert.ToInt32(Console.ReadLine());
            for (int i = 1; i<=10; i++)
            {
                resultado = numero * i;
                Console.WriteLine(numero + "*" + i + "=" + resultado);
            }
        }
    }
}
